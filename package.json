  "main": "index.js",
  "scripts": {
    "build": "npm run -s build:clean && npm run -s build:webpack && npm run -s build:assets && npm run -s build:styleguide",
    "build:clean": "rm -rf ./dist",
    "build:webpack": "webpack --config ./webpack.production.config.js",
    "build:assets": "mkdir -p dist/asset && rsync -rtvqL src/asset/ dist/asset",
    "build:styleguide": "build-storybook -c .storybook -o dist/styleguide",
    "start:dev": "PORT=3000 babel-node bin/app-dev-server",
    "start:styleguide": "start-storybook --dont-track -p 3001 -s src",
    "start": "babel-node bin/app-server",
    "test": "npm run test:lint && npm run test:coverage",
    "test:lint": "eslint src test bin .storybook ./*.js",
    "test:dev": "mocha --opts test/mocha.opts ./test/unit/ ./test/integration",
    "test:dev:watch": "mocha --opts test/mocha.opts ./test/unit/ ./test/integration/ --watch",
    "test:coverage": "babel-node node_modules/isparta/bin/isparta cover _mocha -- --opts test/mocha.opts",
    "changes": "./bin/update-changelog.js ./ --dry-run",
    "prerelease": "npm run test && ./bin/update-changelog.js ./",
    "release": "./bin/bump-release.js ./"
  },
